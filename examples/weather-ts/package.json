{
  "name": "weather-ts",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "npm run types && npm run bundle && npm run componentize",
    "types": "jco types wit -o src/generated",
    "bundle": "webpack",
    "componentize": "jco componentize build/bundled.js --wit wit --world-name weather -o dist/weather.wasm"
  },
  "devDependencies": {
    "@bytecodealliance/componentize-js": "^0.18.5",
    "@bytecodealliance/jco": "^1.15.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.3.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "zod": "^4.1.12"
  }
}
